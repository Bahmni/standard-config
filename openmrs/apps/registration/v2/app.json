{
  "patientSearch": {
    "customAttributes": [
      {
        "translationKey": "REGISTRATION_PATIENT_SEARCH_DROPDOWN_PHONE_NUMBER",
        "fields": ["phoneNumber", "alternatePhoneNumber"],
        "expectedFields": [
            {
              "field": "phoneNumber",
              "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_PHONE_NUMBER"
            }, 
            {
              "field": "alternatePhoneNumber",
              "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_ALTERNATE_PHONE_NUMBER"
            }
        ],
        "type": "person"
      },
      {
        "translationKey": "REGISTRATION_PATIENT_SEARCH_DROPDOWN_EMAIL",
        "fields": ["email"],
        "expectedFields": [
            {
              "field": "email",
              "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_EMAIL"
            }
        ],
        "type": "person"
      },
      {
        "translationKey": "REGISTRATION_PATIENT_SEARCH_DROPDOWN_VILLAGE",
        "fields": ["City_village"],
        "expectedFields": [
            {
              "field": "City_village",
              "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_VILLAGE"
            }
        ],
        "type": "address"
      },
      {
        "translationKey": "REGISTRATION_PATIENT_SEARCH_DROPDOWN_LOCALITY",
        "fields": ["address2"],
        "expectedFields": [
            {
              "field": "address2",
              "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_LOCALITY"
            }
        ],
        "type": "address"
      },
      {
        "translationKey": "REGISTRATION_PATIENT_SEARCH_DROPDOWN_PROGRAM_REGISTRATION_ID",
        "fields": ["ID_Number"],
        "expectedFields": [
            {
              "field": "ID_Number",
              "translationKey": "REGISTRATION_PATIENT_SEARCH_HEADER_PROGRAM_REGISTRATION_ID"
            }
        ],
        "type": "program"
      }
    ]
  },
  "defaultVisitType": "OPD",
  "patientInformation": {
    "defaultIdentifierPrefix": "ABC",
    "patientNameDisplayOrder": ["firstName", "middleName", "lastName"],
    "showBirthTime": true,
    "showDOBEstimated": true,
    "showEnterManually":false,
    "showMiddleName": true,
    "showLastName": true,
    "isFirstNameMandatory": true,
    "isGenderMandatory": false,
    "isDateOfBirthMandatory": true,
    "isLastNameMandatory": true,
    "addressHierarchy": {
      "showAddressFieldsTopDown": true,
      "strictAutocompleteFromLevel": "postalCode"
    },
    "additionalPatientInformation": {
      "translationKey": "REGISTRATION_ADDITIONAL_PATIENT_INFORMATION_TITLE", 
        "expectedFields": [     
            {
              "field": "email",
              "translationKey": "REGISTRATION_PATIENT_SEARCH_DROPDOWN_EMAIL"
            }
        ]
    }
  },
  "fieldValidation": {
    "firstName": {
      "pattern": "^[a-zA-Z\\s]*$",
      "errorMessage": "First name should contain only alphabets without space",
      "required": true
    },
    "middleName": {
      "pattern": "^[a-zA-Z\\s]*$",
      "errorMessage": "Middle name should contain only alphabets without space",
       "required": true
    },
    "lastName": {
      "pattern": "^[a-zA-Z\\s]*$",
      "errorMessage": "Last name should contain only alphabets without space",
      "required": true
    },
    "email": {
      "pattern": "^(?=.{1,40}$)(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,4})$",
      "errorMessage": "Should be a valid email address"
    }
  }
}
